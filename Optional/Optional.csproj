<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <PackageId>MandelV.Optional</PackageId>
    <Company>MandelV</Company>
    <Authors>MandelV</Authors>
    <Description>Provide Optional for C#</Description>
    <Copyright>MandelV</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/MandelV/Optional</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MandelV/Optional.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>c#;Optional;CSharp</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>C#</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
